algoritmo "Escolha"

var
voto : literal
inicio

Escreval("Digite seu voto")
leia(voto)

se voto = "20"  entao
    Escreval("Você votou em Martim King")
senao
se voto = "30"  entao
    Escreval("Você votou em Mariana Santos")
senao
se voto = "40"  entao
    Escreval("Você votou em Samuel Rosa")
senao
    Escreval(" Voto Nulo.")
fimse
fimse
fimse

fimalgoritmo
